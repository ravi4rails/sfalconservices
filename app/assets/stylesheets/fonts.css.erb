@font-face {
  font-family: 'Glyphicons Halflings';

  src: url('<%= asset_path("glyphicons-halflings-regular.eot") %>');
  src: url('<%= asset_path("glyphicons-halflings-regular.eot?#iefix") %>') format('embedded-opentype'), url('<%= asset_path("glyphicons-halflings-regular.woff2") %>') format('woff2'), url('<%= asset_path("glyphicons-halflings-regular.woff") %>') format('woff'), url('<%= asset_path("glyphicons-halflings-regular.ttf") %>') format('truetype'), url('<%= asset_path("glyphicons-halflings-regular.svg#glyphicons_halflingsregular") %>') format('svg');
}

@font-face {
  font-family: 'Simple-Line-Icons';

  src: url('<%= asset_path("Simple-Line-Icons.eot") %>');
  src: url('<%= asset_path("Simple-Line-Icons.eot?#iefix") %>') format('embedded-opentype'), url('<%= asset_path("Simple-Line-Icons.woff") %>') format('woff'), url('<%= asset_path("glyphicons-halflings-regular.woff") %>') format('woff'), url('<%= asset_path("Simple-Line-Icons.ttf") %>') format('truetype'), url('<%= asset_path("Simple-Line-Icons.svg#Simple-Line-Icons") %>') format('svg');
  font-weight: normal;
  font-style: normal;
}

@font-face {
  font-family: 'revicons';

  src: url('<%= asset_path("revicons.eot?5510888") %>');
  src: url('<%= asset_path("revicons.eot?5510888#iefix") %>') format('embedded-opentype'), url('<%= asset_path("revicons.woff?5510888") %>') format('woff'), url('<%= asset_path("revicons.ttf?5510888") %>') format('truetype'), url('<%= asset_path("revicons.svg?5510888#revicons") %>') format('svg');
  font-weight: normal;
  font-style: normal;
}
